{
  "name": "electron-react-typescript-template",
  "version": "0.1.0",
  "private": true,
  "main": "./build/electron/index.js",
  "homepage": "./",
  "scripts": {
    "start:web": "react-scripts start",
    "build:web": "react-scripts build",
    "test:web": "react-scripts test",
    "postinstall": "electron-builder install-app-deps",
    "start": "concurrently \"cross-env BROWSER=none yarn start:web\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "build": "yarn build:web && tsc -p electron && electron-builder",
    "eject:web": "react-scripts eject"
  },
  "author": {
    "name": "Daniel Novacik",
    "email": "novacik.daniel@gmail.com",
    "url": "https://github.com/dnovacik"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "cross-env": "^7.0.3",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^1.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sanitize.css": "^12.0.1",
    "styled-components": "^5.2.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^12.20.7",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "concurrently": "^6.0.0",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "wait-on": "^5.3.0"
  }
}
